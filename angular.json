{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "DailyFace.me": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "fr": "src/locale/messages.fr.xlf",
          "de": "src/locale/messages.de.xlf",
          "it": "src/locale/messages.it.xlf",
          "pt": "src/locale/messages.pt.xlf"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/my-daily-face",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "i18nMissingTranslation": "warning"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kB",
                  "maximumError": "16kB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "serviceWorker": "ngsw-config.json"
            },
            "fr": {
              "aot": true,
              "outputPath": "dist/my-daily-face/fr",
              "i18nLocale": "fr",
              "i18nFormat": "xlf"
            },
            "de": {
              "aot": true,
              "outputPath": "dist/my-daily-face/de",
              "i18nLocale": "de",
              "i18nFormat": "xlf"
            },
            "it": {
              "aot": true,
              "outputPath": "dist/my-daily-face/it",
              "i18nLocale": "it",
              "i18nFormat": "xlf"
            },
            "pt": {
              "aot": true,
              "outputPath": "dist/my-daily-face/pt",
              "i18nLocale": "pt",
              "i18nFormat": "xlf"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "DailyFace.me:build:production"
            },
            "development": {
              "buildTarget": "DailyFace.me:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "src/polyfills.ts",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
